<%- include("partials/header") -%> 

<h1>Compose</h1>
<form action="/compose" method="POST">
    
    <div class="form-group">
        <label for="exampleFormControlInput1">Title</label>
        <input type="text" class="form-control" id="exampleFormControlInput1" name="postTitle">
    </div>
    <div class="form-group">
        <label for="exampleFormControlTextarea1">Post</label>
        <textarea class="form-control" name="postBody" id="exampleFormControlTextarea1" rows="3"></textarea>
    </div>
    <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>

<%- include("partials/footer") -%> 
